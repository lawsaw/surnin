mixin opengraph(options)
    -
        var options = getOptions(options),
        tags = [
            {
                'title': 'title',
                'tag': 'og:title',
            },{
                'title': 'description',
                'tag': 'og:description',
            },{
                'title': 'image',
                'tag': 'og:image',
            },{
                'title': 'url',
                'tag': 'og:url',
            },{
                'title': 'type',
                'tag': 'og:type',
            }
        ];
    -for(var i=0; i< tags.length; i++) {
        meta(
            name= tags[i]['tag']
            content= options[tags[i]['title']]
        )
        meta(
            property= tags[i]['tag']
            content= options[tags[i]['title']]
        )
    -}